{
  "dynamic_templates": [
    {
      "label": {
        "path_match": "label.*",
        "mapping": {
          "analyzer": "termed",
          "type": "text"
        }
      }
    },
    {
      "sortByLabel": {
        "path_match": "sortByLabel.*",
        "mapping": {
          "type": "keyword"
        }
      }
    },
    {
      "vocabularyLabel": {
        "path_match": "vocabulary.label.*",
        "mapping": {
          "index": false,
          "type": "keyword"
        }
      }
    },
    {
      "altLabel": {
        "path_match": "altLabel.*",
        "mapping": {
          "analyzer": "termed",
          "type": "text"
        }
      }
    },
    {
      "definition": {
        "path_match": "definition.*",
        "mapping": {
          "analyzer": "termed",
          "type": "text"
        }
      }
    },
    {
      "references.contributor.id": {
        "path_match": "references.contributor.id.*",
        "mapping": {
          "type": "keyword"
        }
      }
    }
  ],
  "properties": {
    "broader": {
      "type": "keyword"
    },
    "hasNarrower": {
      "type": "boolean"
    },
    "definedInScheme": {
      "type": "keyword"
    },
    "usedInScheme": {
      "type": "keyword"
    },
    "id": {
      "type": "keyword"
    },
    "modified": {
      "type": "date",
      "index": false
    },
    "narrower": {
      "type": "keyword",
      "index": false
    },
    "uri": {
      "type": "keyword"
    },
    "references.contributor.id": {
      "type": "keyword"
    },
    "vocabulary": {
      "properties": {
        "id": {
          "type": "keyword"
        },
        "code": {
          "type": "text",
          "fields": {
            "keyword": {
              "type": "keyword",
              "ignore_above": 256
            }
          }
        },
        "createdBy": {
          "type": "text",
          "fields": {
            "keyword": {
              "type": "keyword",
              "ignore_above": 256
            }
          }
        },
        "createdDate": {
          "type": "date"
        }
      }
    }
  }
}
